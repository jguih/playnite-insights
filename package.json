{
  "name": "playnite-insights-dev",
  "private": true,
  "version": "2.2.0",
  "type": "module",
  "scripts": {
    "dev": "docker compose up --watch --force-recreate --build app-dev",
    "prod:down": "docker compose down app-prod",
    "prod:up": "COMPOSE_BAKE=true docker compose up -d --build app-prod",
    "prod": "npm run prod:down ; npm run prod:up",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test:unit": "COMPOSE_BAKE=true docker compose run --rm --build app-vitest",
    "test": "npm run test:unit",
    "preversion": "npm run test"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.0.0",
    "@types/busboy": "^1.5.4",
    "@types/node": "^24.0.4",
    "@vitest/browser": "^3.2.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.2.6",
    "vitest": "^3.2.3",
    "vitest-browser-svelte": "^0.1.0"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@inlang/paraglide-js": "^2.0.0",
    "@lucide/svelte": "^0.525.0",
    "busboy": "^1.6.0",
    "echarts": "^5.6.0",
    "svelte-tiny-virtual-list": "^2.1.2",
    "zod": "^3.25.74"
  }
}